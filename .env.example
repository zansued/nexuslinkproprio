# Variáveis de ambiente para o NexusLink
# Copie este arquivo para .env.local e preencha os valores

# Configuração da API
VITE_API_URL=http://localhost:3000/api
VITE_APP_ID=nexuslink-app
VITE_API_TOKEN=

# Configurações de SEO
VITE_DEFAULT_DOMAIN=example.com
VITE_ANALYTICS_ID=

# Configurações de Email (opcional)
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=
VITE_SMTP_PASS=

# Configurações de Proxy (opcional)
VITE_PROXY_API_KEY=
VITE_PROXY_SERVICE_URL=
